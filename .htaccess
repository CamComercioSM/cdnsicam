
RewriteOptions inherit
RewriteEngine On
## Is the request for a non-existent file?
RewriteCond "%{REQUEST_FILENAME}" "!-f"
RewriteCond "%{REQUEST_FILENAME}" "!-d"
#RewriteRule ^(.*)$ index.php?$1=$2 [L,QSA]
#Access-Control-Allow-Origin "*" 
#Access-Control-Allow-Origin "https://libs.tiendasicam32.net/"
#Access-Control-Allow-Origin "https://tiendasicam32.net/"
#Access-Control-Allow-Origin "https://www.tiendasicam32.net/"

<IfModule mod_headers.c>
  <FilesMatch "\.(ttf|ttc|otf|eot|woff|woff2|font.css|css|js|json)$">
    Header set Access-Control-Allow-Origin "*"
  </FilesMatch>
</IfModule>

# Apache config
<FilesMatch ".(ttf|ttc|otf|eot|woff|woff2|font.css|css|js|json)">
    Header set Access-Control-Allow-Origin "*"
</FilesMatch>


