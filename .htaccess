# Evitar redirecciones innecesarias en Cloudflare
RewriteEngine On

# Solo redirigir si no es un archivo est√°tico
RewriteCond %{REQUEST_URI} !^/cdnsicam/ [NC]
RewriteCond %{REQUEST_URI} !^/(css|js|ia|img|svg|fonts|videos|audios|plugins|letras|plantillas|scripts|utilidades|errores)/ [NC]
RewriteCond %{REQUEST_URI} !\.(css|js|jpg|png|gif|ico|svg|woff|woff2|ttf|eot|json|xml|txt)$ [NC]
RewriteRule ^(.*)$ /cdnsicam/$1 [L,R=302]

